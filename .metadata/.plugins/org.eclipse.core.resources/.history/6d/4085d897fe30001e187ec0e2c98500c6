package com.teamproject.teamproject.board;

public class BoardNo {
	private int tp_b_no;
	
	public BoardNo() {
		// TODO Auto-generated constructor stub
	}

	public BoardNo(int tp_b_no) {
		super();
		this.tp_b_no = tp_b_no;
	}

	public int getTp_b_no() {
		return tp_b_no;
	}

	public void setTp_b_no(int tp_b_no) {
		this.tp_b_no = tp_b_no;
	}
	
	

}
